{
  "name": "ticket-booking-system-backend",
  "version": "1.0.0",
  "description": "Concurrency-safe ticket booking system with Node.js, Express, and PostgreSQL",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/migrations/runMigrations.js && node src/index.js",
    "dev": "nodemon --watch src --exec node src/index.js",
    "migrate": "node src/migrations/runMigrations.js",
    "test:concurrent": "node tests/concurrentTest.js"
  },
  "keywords": ["booking", "tickets", "postgresql", "concurrency"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
